#=======================================================================
#
#  TERMCAP for STEVIE, Ng and Demacs
#					Last modified: Nov 22, 1991
#=======================================================================

#=======================================================================
#  Generic terminals
#=======================================================================

#-----------------------------------------------------------------------
#  ANSI terminal
#-----------------------------------------------------------------------
ansi|ANSI terminal:\
	:co#80:li#24:am:bs:pt:bl=^G:le=^H:do=^J:\
	:cl=\E[2J:ce=\E[K:cd=\E[J:sc=\E[s:rc=\E[u:\
	:cm=\E[%i%d;%dH:ho=\E[H:ll=\E[24;1H:\
	:up=\E[A:nd=\E[C:ri=\E[C:\
	:UP=\E[%dA:DO=\E[%dB:RI=\E[%dC:LE=\E[%dD:\
	:al=\E[L:dl=\E[M:AL=\E[%dL:DL=\E[%dM:\
	:ic=\E[@:dc=\E[P:IC=\E[%d@:DC=\E[%dP:\
	:cs=\E[%i%d;%dr:im=\E[4h:ei=\E[4l:\
        :so=\E[7m:se=\E[m:us=\E[4m:ue=\E[m:\
	:mb=\E[5m:md=\E[1m:mr=\E[7m:mh=\E[2m:me=\E[m:

#-----------------------------------------------------------------------
#  DEC VT100 terminal
#-----------------------------------------------------------------------
vt100|DEC VT100:\
	:sc=\E7:rc=\E8:ti=4\E(B\E)0:as=^N:ae=^O:\
	:al@:dl@:AL@:DL@:ic@:dc@:IC@:DC@:im@:ei@:\
	:xn:xo:vt#3:tc=ansi:

#-----------------------------------------------------------------------
#  DEC VT52 terminal
#-----------------------------------------------------------------------
vt52|DEC VT52:\
	:co#80:li#24:bs:pt:le=^H:do=^J:\
	:cl=\EH\EJ:ce=\EK:cd=\EJ:\
	:ho=\EH:cm=\EY%+ %+ :\
	:up=\EA:do=\EB:le=\ED:ri=\EC:nd=\EC:\
	:sr=\EI:as=\EF:ae=\EG:

#=======================================================================
#  MS/PC-DOS terminals
#=======================================================================

#-----------------------------------------------------------------------
#  NEC PC-9801 series
#-----------------------------------------------------------------------
pc98|pc9801|NEC PC-9801:\
	:vi=\E[>5h:vs=\E[>5l:ve=\E[>5l:\
        :xn:cs@:im@:ei@:ic@:IC@:dc@:DC@:do=\E[B:tc=ansi:

#-----------------------------------------------------------------------
#  NEC PC-9801 series - cyan reverse, yellow underline
#-----------------------------------------------------------------------
pc98-c|pc9801-c|NEC PC-9801:\
	:so=\E[46m:se=\E[m:mr=\E[46m:\
	:us=\E[33;4m:tc=pc98:

#-----------------------------------------------------------------------
#  NEC PC-9801 series - 25 line mode
#-----------------------------------------------------------------------
pc98-25|pc9801-25|NEC PC-9801 25 line:\
	:li#25:ll=\E[25;1H:\
	:ti=\E[>1h:te=\E[>1l:tc=pc98:

#-----------------------------------------------------------------------
#  Toshiba J3100 with Japanese DOS (much like PC-9801)
#-----------------------------------------------------------------------
j31|j3100|Toshiba J-3100:\
	:li#25:in:km:\
	:le=\E[D:do=\E[B:\
	:ic@:dc@:IC@:DC@:sc@:rc@:cs@:im@:ei@:\
	:tc=ansi:

#-----------------------------------------------------------------------
#  AX machines with ANSIJ.SYS, or Toshiba J3100 with English DOS
#-----------------------------------------------------------------------
ax|AX machines:\
	:vi=\E[>5h:vs=\E[>5l:ve=\E[>5l:\
	:cs@:sf@:sr@:ic@:dc@:IC@:DC@:im@:ei@:tc=ansi:

#-----------------------------------------------------------------------
#  Fujitsu FMR series and FM-Towns (MS-DOS)
#-----------------------------------------------------------------------
fmr|towns|Fujitsu FMR and FM-Towns:\
	:ce=\ET:cd=\EY:al=\EE:dl=\ER:\
	:vi=\E[1v:vs=\E[0v:ve=\E[0v:\
	:AL@:DL@:cs@:sf@:sr@:ic@:dc@:IC@:DC@:im@:ei@:\
	:xn:tc=ansi:

#-----------------------------------------------------------------------
#  OKI if-800 model 60 (incomplete and very doubtful)
#-----------------------------------------------------------------------
if800|if-800|if-800 model 60:\
	:vi=\E0:vs=\E1:ve=\E1:\
	:tc=ansi:

#-----------------------------------------------------------------------
#  IBM-PC with ANSI.SYS
#-----------------------------------------------------------------------
ibmpc|ibmpc-ansi|ANSI.SYS:\
	:li#25:co#80:bs:pt:km:bl=^G:\
	:le=\E[D:do=\E[B:\
	:al@:dl@:AL@:DL@:ic@:dc@:IC@:DC@:im@:ei@:\
	:cs@:cd@:tc=ansi:

#-----------------------------------------------------------------------
#  IBM-PC with NANSI.SYS or ZANSI.SYS
#-----------------------------------------------------------------------
nansi|zansi|ibmpc-nansi|ibmpc-zansi|NANSI or ZANSI.SYS:\
	:li#25:co#80:bs:pt:bl=^G:le=^H:do=^J:\
	:cs@:cd@:im@:ei@:tc=ansi:

#-----------------------------------------------------------------------
#  IBM-PC with NNANSI.SYS
#-----------------------------------------------------------------------
nnansi|ibmpc-nnansi|NNANSI.SYS:\
	:li#25:co#80:bs:pt:bl=^G:le=^H:do=^J:\
	:cs@:im@:ei@:tc=ansi:

#=======================================================================
#  Miscellaneous
#=======================================================================

#-----------------------------------------------------------------------
#  OS/2 on PC-9801, IBM PS/55 (like IBM-PC with ANSI.SYS)
#-----------------------------------------------------------------------
os2|OS2:\
	:li#25:co#80:bs:pt:bl=^G:le=^H:do=^J:\
	:al@:dl@:AL@:DL@:ic@:dc@:IC@:DC@:\
	:cs@:cd@:im@:ei@:tc=ansi:

#-----------------------------------------------------------------------
#  Atari ST
#-----------------------------------------------------------------------
atari|Atari ST:\
	:tc=vt52:

#-----------------------------------------------------------------------
#  Commodore AMIGA
#-----------------------------------------------------------------------
amiga|AMIGA:\
	:tc=vt100:

#-----------------------------------------------------------------------
#  Hardcopy terminal
#-----------------------------------------------------------------------
dumb:\
	:co#80:do=^J:nl=^M^J:bl=^G:am:
